<% if logged_in? %>
      <nav class="navbar navbar-expand-lg btn-group w-100" role="group" aria-label="Basic outlined example">
        <%= link_to "マイページへ", user_path(current_user.id), class: 'btn btn-outline-primary btn-lg' %>
        <%= link_to "ログアウト", session_path(current_user.id), method: :delete, class: 'btn btn-outline-primary btn-lg' %>
      </nav>    
<% else %>
      <nav class="navbar navbar-expand-lg btn-group w-100" role="group" aria-label="Basic outlined example">
        <%= link_to "新規登録する", new_user_path, class: 'btn btn-outline-primary btn-lg' %>
        <%= link_to "ログイン", new_session_path, class: 'btn btn-outline-primary btn-lg' %>
      </nav>
<% end %>